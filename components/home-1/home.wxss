/* components/home/home.wxss */
.box{
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-around;
}
.place{
  width: 100rpx;
  height: 100%;
  display: flex;
  font-size: 30rpx;
  align-items: center;
}
.place image{
  width: 30rpx;
  height: 20rpx;
}
.serch{
  width: 480rpx;
  height: 90%;
  border-radius: 60rpx;
  position: relative;
  background-color: whitesmoke;
}
.serch image{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  left: 38rpx;
  top: 25rpx;
}
.serch input{
  width: 350rpx;
  height: 50rpx;
  font-size: 27rpx;
margin-left: 70rpx;
margin-top: 15rpx;

}
.user{
  width: 100rpx;
  height: 100%;
  font-size: 25rpx;
  text-align: center;
}
.user image{
  width: 40rpx;
  height: 45rpx;

}